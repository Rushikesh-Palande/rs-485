{
  "identifier": "default",
  "description": "Default capability set for RS-485 Command Center",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:allow-spawn",
    "shell:allow-kill",
    "shell:allow-open",
    "dialog:default",
    "fs:default",
    "core:window:allow-create",
    "core:window:allow-get-all-windows",
    "core:window:allow-show",
    "core:window:allow-close",
    "core:window:allow-set-focus",
    "core:webview:allow-create-webview-window"
  ]
}
